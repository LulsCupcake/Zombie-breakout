<?xml version="1.0"?>
<dynamic_object>
	<diesel materials="units/payday2/characters/fbi_boss_1/fbi_boss_1" orientation_object="root_point" />

	<decal_surfaces default_material="flesh" />

	<graphics>
		<graphic_group name="character" enabled="true" culling_object="g_body">

			<lod_object name="lod_body">
				<object name="g_body" 		enabled="true"  max_distance="2000"  max_draw_lod="0" cpy_material_vars="contour_color, contour_opacity">
					<material materials="mtr_body" max_distance="1000" />
					<material materials="mtr_body_lod_1" max_distance="1500" />
					<material materials="mtr_body_lod_2" />
				</object>
				<object name="g_body_lod1" 	enabled="true" lod="1" >
					<material materials="mtr_body" max_distance="0" cpy_material_vars="contour_color, contour_opacity"/>
					<material materials="mtr_body_lod_2" />
				</object>
			</lod_object>

			<lod_object name="lod_head">
				<object name="g_head" 		enabled="true"  max_distance="2000"  max_draw_lod="0" cpy_material_vars="contour_color, contour_opacity">
					<material materials="mtr_head" max_distance="1000" />
					<material materials="mtr_head_lod_1" max_distance="1500" />
					<material materials="mtr_head_lod_2" />
				</object>
				<object name="g_head_lod1" 	enabled="true" lod="1" >
					<material materials="mtr_head" max_distance="0" cpy_material_vars="contour_color, contour_opacity"/>
					<material materials="mtr_head_lod_2" />
				</object>
			</lod_object>
			<object name="s_body" enabled="false" shadow_caster="true"/>
			
		</graphic_group>
	</graphics>

</dynamic_object>